
@{
    ViewData["Title"] = "BookList";
}

<h1>BookList</h1>
<table class="table table-bordered">
    <thead>
        <tr>
            <td>Id</td>
            <td>Name</td>
            <td>Price</td>
        </tr>
    </thead>
    <tbody>
        @foreach(Book b in (IEnumerable<Book>)ViewData["List"])
        {
            <tr>
                <td>@b.Id</td>
                <td>@b.Name</td>
                <td>@b.Price</td>
            </tr>
        }
    </tbody>
</table>
